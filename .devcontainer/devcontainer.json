{
	"name": "Ruby on Rails & Postgres",
	"dockerComposeFile": "docker-compose.yml",
	"service": "app",
	"workspaceFolder": "/workspace",
	"customizations": {
		"vscode": {
			"extensions": [
				"rebornix.Ruby"
			]
		}
	},
	"forwardPorts": [3000, 5432],
	"postCreateCommand": "bundle install && rails db:setup",
	"remoteUser": "vscode"
}